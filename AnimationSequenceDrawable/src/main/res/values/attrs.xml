<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AnimationImageView">
        <attr name="loopCount" format="integer" />
        <attr name="loopBehavior">
            <flag name="loop_finite" value="1"/>
            <flag name="loop_inf" value="2"/>
            <flag name="loop_default" value="3"/>

        </attr>
        <attr name="srcType" >
            <flag name="webp" value="1"/>
            <flag name="gif" value="2"/>
        </attr>
    </declare-styleable>
</resources>